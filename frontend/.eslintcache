t();\n      const { username, email } = e.target;\n\n      await fetch('http://localhost:8000/auth/create_account', {\n        method: 'POST',\n        body: JSON.stringify({\n          username: username.value,\n          email: email.value,\n        }),\n        headers: { 'Content-Type': 'application/json' },\n      }).then((res) => res.json())\n        .then((data) => {\n          console.log(data.userid);\n          this.handleBooking(data.userid);\n        });\n    }\n\n    handleBooking = (userid) => {\n      console.log('make booking');\n      console.log(userid);\n\n      fetch('http://localhost:8000/booking', {\n        method: 'POST',\n        body: JSON.stringify({\n          userID: userid,\n          timeslotID: this.state.timeslotID,\n          tableID: this.state.tableID,\n        }),\n        headers: { 'Content-Type': 'application/json' },\n      }).then((response) => (response.status === 200 ? this.setState({ bookingDone: true }) : this.setState({ bookingError: true })));\n    }\n\n    render() {\n      return (\n        <div>\n          <Navbar />\n          <div className=\"main\">\n            <div>\n              <Link to=\"/booking/date\">Date</Link>\n              <Link to=\"/booking/time\">Timeslot</Link>\n              <Link to=\"/booking/groupsize\">Group Size</Link>\n              <Link to=\"/booking/tables\">Tables</Link>\n            </div>\n            <div>\n              <h2>Confirm booking in behalf of customer</h2>\n              {this.state.bookingError === true ? (<div>Oops something went wrong</div>)\n                : (<div />)}\n              {this.state.bookingDone === true ? (<div>You booked a table in behalf of a customer! <br /> We will send the customer an email shortly to</div>)\n                : (\n                  <div>\n                    <div>People: {this.state.groupsize}</div>\n                    <div>Time: {this.state.startTime} to {this.state.endTime}</div>\n                    <div>Date: {this.state.date}</div>\n                    <div>Table: {this.state.tableID}</div>\n                    <form onSubmit={this.handleAddedData}>\n                      <label htmlFor=\"username\">Username</label>\n                      <input type=\"text\" id=\"username\" name=\"username\" placeholder=\"username\" />\n                      <label htmlFor=\"email\">Email</label>\n                      <input type=\"email\" id=\"email\" name=\"email\" placeholder=\"email\" />\n                      <button type=\"submit\">Update</button>\n                    </form>\n                  </div>\n                )}\n            </div>\n          </div>\n        </div>\n      );\n    }\n}\n","/Users/sariguci/Documents/react project/frontend/src/pages/AddTables.js",["211","212","213","214"],"/Users/sariguci/Documents/react project/frontend/src/components/AddTableForm.js",["215","216"],"import React, { Component } from 'react';\n\nexport default class AddTableForm extends Component {\n  render() {\n    return (\n      <form onSubmit={this.props.onAddTable}>\n        <label htmlFor=\"name\">Name</label>\n        <input type=\"text\" id=\"name\" name=\"name\" placeholder=\"table name\" />\n        <label htmlFor=\"capacity\">Capacity</label>\n        <input type=\"text\" name=\"capacity\" placeholder=\"capacity\" />\n        <button>Add Table</button>\n      </form>\n    );\n  }\n}\n",{"ruleId":"217","severity":1,"message":"218","line":31,"column":32,"nodeType":"219","endLine":31,"endColumn":46},{"ruleId":"220","replacedBy":"221"},{"ruleId":"222","replacedBy":"223"},{"ruleId":"224","replacedBy":"225"},{"ruleId":"226","replacedBy":"227"},{"ruleId":"228","replacedBy":"229"},{"ruleId":"230","replacedBy":"231"},{"ruleId":"232","severity":1,"message":"233","line":18,"column":15,"nodeType":"234","messageId":"235","endLine":18,"endColumn":108},{"ruleId":"232","severity":1,"message":"233","line":18,"column":15,"nodeType":"234","messageId":"235","endLine":18,"endColumn":108},{"ruleId":"217","severity":1,"